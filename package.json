{
  "name": "@usdtg/monorepo",
  "private": true,
  "version": "0.5.0",
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "contracts-*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently -n DAPP,SITE,API,ORACLE \"pnpm --dir apps/dapp dev\" \"pnpm --dir apps/site dev\" \"pnpm --dir services/api dev\" \"pnpm --dir services/oracle dev\"",
    "dev:dapp": "pnpm --dir apps/dapp dev",
    "dev:site": "pnpm --dir apps/site dev",
    "dev:api": "pnpm --dir services/api dev",
    "dev:oracle": "pnpm --dir services/oracle dev",
    "build": "pnpm -r run build",
    "test": "pnpm --dir contracts-evm test"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@improbable-eng/grpc-web": "^0.15.0",
    "protobufjs": "^7.5.4"
  }
}